    <inertial>
      <mass value="0.207"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  
  <link name="link4">
    <visual>
      <geometry>
        <mesh filename="package://al5d_description/meshes/link4.stl" scale="0.001 0.001 0.001"/>
